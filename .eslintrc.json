{
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "extends": [
    "next/core-web-vitals",
    "plugin:@typescript-eslint/recommended",
    "next"
  ],
  "rules": {
    "no-unused-vars":"warn",
    "no-console": "warn",
    "semi": ["error", "always"],
    "quotes": ["error", "double"],
    "indent": ["error", 2],
    "no-multiple-empty-lines": ["error", { "max": 1 }]
  }
}